    <queryString language="SQL">
        <![CDATA[SELECT
    c.id AS category_id,
    c.name AS category_name,
    p.id AS product_id,
    p.name AS product_name,
    p.description AS product_description,
    p.price AS product_price,
    p.quantity AS product_quantity,
    p.created_at AS product_created_at
